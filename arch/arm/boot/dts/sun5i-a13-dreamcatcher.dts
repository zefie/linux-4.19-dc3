			clocks = <0x14>;
			compatible = "allwinner,sun5i-a13-apb1-gates-clk";
			linux,phandle = <0x2d>;
			phandle = <0x2d>;
			reg = <0x1c2006c 0x4>;
		};

		clk@01c20080 {
