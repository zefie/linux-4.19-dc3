			compatible = "allwinner,sun4i-a10-tcon-ch0-clk";
			linux,phandle = <0x4>;
			phandle = <0x4>;
			reg = <0x1c20118 0x4>;
		};

		clk@01c2012c {
			#clock-cells = <0x0>;
			clock-output-names = "tcon-ch1-sclk";
			clocks = <0x9 0xa 0x15 0x16>;
			compatible = "allwinner,sun4i-a10-tcon-ch1-clk";
			linux,phandle = <0x32>;
			phandle = <0x32>;
			reg = <0x1c2012c 0x4>;
		};

		clk@01c20140 {
			#clock-cells = <0x0>;
			clock-output-names = "codec";
			clocks = <0x12 0x0>;
			compatible = "allwinner,sun4i-a10-codec-clk";
			linux,phandle = <0x2c>;
			phandle = <0x2c>;
			reg = <0x1c20140 0x4>;
